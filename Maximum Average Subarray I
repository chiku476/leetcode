class Solution {
    public double findMaxAverage(int[] nums, int k) {
        double maxAvg=0;
        int sum=0;
        for(int i=0;i<k;i++){
            sum=sum+nums[i];
         }
         maxAvg=(double)sum/k;
        for(int i=1;i<=nums.length-k;i++){
            maxAvg = Math.max(maxAvg,((double) sum-nums[i-1]+nums[k-1+i])/k);
        }
        return Math.round(maxAvg * 100000.0) / 100000.0;
    }
}
